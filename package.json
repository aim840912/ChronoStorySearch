{
  "name": "maplestory",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "concurrently \"npm:dev:next\" \"npm:watch:images\"",
    "dev:next": "next dev --turbopack",
    "watch:images": "node scripts/watch-images.js",
    "build": "next build --turbopack",
    "start": "next start",
    "lint": "eslint",
    "type-check": "tsc --noEmit",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "generate-images": "node scripts/generate-image-manifest.js",
    "r2:upload": "bash scripts/upload-to-r2-wrangler.sh",
    "r2:sync": "~/rclone copy public/images r2:maplestory-images/images --progress --transfers=16 --checkers=16",
    "r2:list": "~/rclone ls r2:maplestory-images/images | head -20",
    "r2:check": "~/rclone check public/images r2:maplestory-images/images",
    "vercel:pause": "node scripts/vercel-manage.js pause",
    "vercel:unpause": "node scripts/vercel-manage.js unpause",
    "vercel:status": "node scripts/vercel-manage.js status"
  },
  "dependencies": {
    "@vercel/analytics": "^1.5.0",
    "next": "^15.5.6",
    "react": "^19.2.0",
    "react-dom": "^19.2.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4.1.15",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "chokidar": "^4.0.3",
    "concurrently": "^9.2.1",
    "eslint": "^9.38.0",
    "eslint-config-next": "^15.5.6",
    "tailwindcss": "^4.1.15",
    "typescript": "5.9.3",
    "wrangler": "^4.43.0"
  }
}
